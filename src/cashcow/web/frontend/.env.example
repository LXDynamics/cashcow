# CashCow Frontend Environment Variables

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_WS_BASE_URL=ws://localhost:8000/ws

# Development Mode
# Set to 'true' to use mock data instead of real API
REACT_APP_MOCK_MODE=true

# Application Settings
REACT_APP_APP_NAME=CashCow
REACT_APP_VERSION=1.0.0

# Theme Settings
REACT_APP_THEME_MODE=light
REACT_APP_PRIMARY_COLOR=#1976d2
REACT_APP_SECONDARY_COLOR=#dc004e

# Feature Flags
REACT_APP_ENABLE_WEBSOCKETS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false